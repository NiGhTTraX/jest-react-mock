{
  "name": "jest-react-mock",
  "version": "2.0.1",
  "description": "jest helper for react-mock-component",
  "keywords": [
    "jest",
    "react",
    "mock",
    "tdd",
    "testing",
    "expect"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NiGhTTraX/jest-react-mock.git"
  },
  "author": "Andrei Picus",
  "license": "MIT",
  "homepage": "https://github.com/NiGhTTraX/jest-react-mock",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn run clean && yarn run compile",
    "clean": "rm -rf ./dist",
    "compile": "tsc -p tsconfig.build.json",
    "prepublishOnly": "yarn run build",
    "release": "standard-version",
    "lint": "eslint --ext ts,tsx,js .",
    "test": "jest --config tests/jest.config.js --coverage",
    "report-coverage": "codecov"
  },
  "peerDependencies": {
    "@types/jest": "^26.0.0",
    "jest": "^26.0.1",
    "react-mock-component": "^2.3.0"
  },
  "devDependencies": {
    "@nighttrax/eslint-config-tsx": "~8.0.0",
    "@tdd-buffet/jest-config": "~3.0.0",
    "@tdd-buffet/react": "~1.5.0",
    "@tdd-buffet/tsconfig": "~1.0.4",
    "@types/jest": "~26.0.0",
    "codecov": "~3.8.0",
    "eslint": "~7.25.0",
    "jest": "^26.0.1",
    "react": "~17.0.0",
    "react-dom": "~17.0.0",
    "react-mock-component": "^2.3.0",
    "standard-version": "~9.3.0",
    "strip-ansi": "~6.0.0",
    "typescript": "~4.2.0"
  }
}
