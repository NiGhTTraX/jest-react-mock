{
  "name": "jest-react-mock",
  "version": "3.1.1",
  "description": "jest helper for react-mock-component",
  "keywords": [
    "jest",
    "react",
    "mock",
    "tdd",
    "testing",
    "expect"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NiGhTTraX/jest-react-mock.git"
  },
  "author": "Andrei Picus",
  "license": "MIT",
  "homepage": "https://github.com/NiGhTTraX/jest-react-mock",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm run clean && pnpm run compile",
    "clean": "rm -rf ./dist",
    "compile": "tsc -p tsconfig.build.json",
    "prepublishOnly": "pnpm run build",
    "release": "standard-version",
    "lint": "eslint --ext ts,tsx,js .",
    "test": "jest --config tests/jest.config.js --coverage",
    "report-coverage": "codecov"
  },
  "peerDependencies": {
    "@types/jest": "^26.0.0 || ^27.0.0 || ^28.0.0 || ^29.0.0",
    "jest": "^26.0.1 || ^27.0.0 || ^28.0.0 || ^29.0.0",
    "react-mock-component": "^3.0.0"
  },
  "devDependencies": {
    "@nighttrax/eslint-config-tsx": "~10.0.0-beta.2",
    "@tdd-buffet/jest-config": "~5.0.0",
    "@tdd-buffet/tsconfig": "~1.0.4",
    "@testing-library/react": "~13.4.0",
    "@types/jest": "~29.2.0",
    "@types/react": "~18.0.15",
    "codecov": "~3.8.0",
    "eslint": "~8.25.0",
    "jest": "~29.2.0",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "react-mock-component": "~3.1.0",
    "standard-version": "~9.5.0",
    "strip-ansi": "~6.0.0",
    "typescript": "~4.8.0"
  }
}
